QLineEdit, QComboBox {
  padding: 9px 12px;
  border-radius: 10px;
  border: 1px solid rgba(255,255,255,0.14);
  background: #121318;
}
QLineEdit:focus, QComboBox:focus {
  border-color: #2a66ff;
  background: #151826;
}
QCheckBox { spacing: 8px; }
QCheckBox::indicator { width: 16px; height: 16px; }
